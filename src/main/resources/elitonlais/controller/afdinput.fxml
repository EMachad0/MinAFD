<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="768.0" prefWidth="1366.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="elitonlais.controller.AFDInputController">
    <Label layoutX="50.0" layoutY="20.0" prefHeight="25.0" prefWidth="150.0" text="Alfabeto" textAlignment="CENTER" />
    <Label layoutX="50.0" layoutY="50.0" prefHeight="25.0" prefWidth="150.0" text="Numero De Estados" textAlignment="CENTER" />
    <TextField fx:id="fieldAlfabeto" layoutX="200.0" layoutY="20.0" prefHeight="25.0" prefWidth="390.0" />
    <TextField fx:id="fieldNumState" layoutX="200.0" layoutY="50.0" prefHeight="25.0" prefWidth="390.0" />
    <Button fx:id="btnTabela" layoutX="220.0" layoutY="85.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="200.0" text="Gerar Tabela">
        <font>
            <Font name="Calibri" size="18.0" />
        </font>
    </Button>
    <ScrollPane hmax="0.0" layoutX="47.0" layoutY="140.0" prefHeight="303.0" prefWidth="543.0">
        <TableView fx:id="myTable" layoutX="50.0" prefHeight="300.0" prefWidth="540.0">
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
        </TableView>
    </ScrollPane>
    <Label layoutX="50.0" layoutY="450.0" prefHeight="25.0" prefWidth="150.0" text="Estado Inicial" textAlignment="CENTER" />
    <Label layoutX="50.0" layoutY="480.0" prefHeight="25.0" prefWidth="150.0" text="Estados Finais" textAlignment="CENTER" />
    <TextField fx:id="fieldEstadoInicial" layoutX="200.0" layoutY="450.0" prefHeight="25.0" prefWidth="390.0" />
    <TextField fx:id="fieldEstadosFinais" layoutX="200.0" layoutY="480.0" prefHeight="25.0" prefWidth="390.0" />
    <Button fx:id="btnSimplifica" layoutX="220.0" layoutY="520.0" mnemonicParsing="false" prefHeight="34.0" prefWidth="200.0" text="Simplificar" />
</AnchorPane>
